#include <iostream>
using namespace std;
int main()
{
	for (int a = 1;a < 16;++a) {
		switch (a) {

		case 1:
			cout << "message # 1" << endl;
			break;

		case 2:
			cout << "message # 2" << endl;
			break;

		case 3:
			cout << "message # 3" << endl;
			break;

		case 4:
			cout << "message # 4" << endl;
			break;

		case 5:
			cout << "message # 5" << endl;
			break;

		case 6:
			cout << "message # 6" << endl;
			break;

		case 7:
			cout << "message # 7" << endl;
			break;

		case 8:
			cout << "message # 8" << endl;
			break;

		case 9:
			cout << "message # 9" << endl;
			break;

		case 10:
			cout << "message # 10" << endl;
			break;

		case 11:
			cout << "message # 11" << endl;
			break;

		case 12:
			cout << "message # 12" << endl;
			break;

		case 13:
			cout << "message # 13" << endl;
			break;

		case 14:
			cout << "message # 14" << endl;
			break;

		case 15:
			cout << "message # 15" << endl;
			break;
		}
	}
	system("pause");
	return 0;
}
